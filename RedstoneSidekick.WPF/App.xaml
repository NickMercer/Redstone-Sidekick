<Application x:Class="RedstoneSidekick.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RedstoneSidekick.WPF"
             StartupUri="ProjectWindow/ProjectWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/RedstoneSidekick.WPF;component/RedstoneSidekickResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="unused" />
        </ResourceDictionary>

    </Application.Resources>
</Application>
