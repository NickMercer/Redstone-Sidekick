<Application x:Class="RedstoneSidekickWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RedstoneSidekickWPF"
             StartupUri="ProjectWindow/ProjectWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/RedstoneSidekickWPF;component/RedstoneSidekickResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="unused" />
        </ResourceDictionary>

    </Application.Resources>
</Application>
